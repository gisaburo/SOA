variant: fcos
version: 1.0.0
passwd:
  users:
    - name: core
      ssh_authorized_keys:
        - ssh-rsa  fedora@localhost.localdomain
storage:
  files:
# kubeletをインストールする
    - path: /root/bin/kubelet
      mode: 0755
      overwrite: true
      contents:
        source: https://storage.googleapis.com/kubernetes-release/release/v1.17.2/bin/linux/amd64/kubelet
# kubeadmをインストールする
    - path: /root/bin/kubeadm
      mode: 0755
      overwrite: true
      contents:
        source: https://storage.googleapis.com/kubernetes-release/release/v1.17.2/bin/linux/amd64/kubeadm
